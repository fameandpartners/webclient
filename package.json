{
  "name": "webclient",
  "version": "0.1.0",
  "license": "MIT",
  "browserslist": "last 4 versions, > 1%, not dead, not ie < 10, Firefox ESR",
  "scripts": {
    "start": "node ./scripts/fame start",
    "start:debug": "node ./scripts/fame start --inspect --inspect-port 9230",
    "build": "node ./scripts/fame build",
    "build:prod": "NODE_ENV=production node ./scripts/fame build",
    "test": "jest",
    "start:prod": "NODE_ENV=production node build/server.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "clean": "rm -rf ./build && rm -rf ./node_modules && yarn && yarn start"
  },
  "engines": {
    "node": ">= 8.11.x"
  },
  "typings": "src/typings/index",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.ts",
      "^@containers(.*)$": "<rootDir>/src/common/containers$1",
      "^@components(.*)$": "<rootDir>/src/common/components$1",
      "^@constants(.*)$": "<rootDir>/src/common/constants$1",
      "^@services(.*)$": "<rootDir>/src/common/services$1",
      "^@transforms(.*)$": "<rootDir>/src/common/transforms$1",
      "^@translations(.*)$": "<rootDir>/src/common/translations$1",
      "^@svg(.*)$": "<rootDir>/src/common/assets/svg$1",
      "^@scss(.*)$": "<rootDir>/src/common/assets/scss$1",
      "^@common(.*)$": "<rootDir>/src/common$1",
      "^@typings(.*)$": "<rootDir>/src/typings$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      },
      "__FAME_CONFIG__": {
        "URLS": {
          "en-US": {
            "api": "http://localhost:3000",
            "frontend": "http://localhost:3002",
            "pcApi": "https://product-catalog-qa4.fameandgroups.com",
            "renderUrl": "https://s3.amazonaws.com/fame-product-renders-dev",
            "renderFallbackUrl": "https://9v2re8nkt4.execute-api.us-east-1.amazonaws.com/dev/ImagePreview"
          },
          "en-AU": {
            "api": "http://au.lvh.me:3000",
            "frontend": "http://au.lvh.me:3002",
            "pcApi": "https://product-catalog-qa4.fameandgroups.com",
            "renderUrl": "https://s3.amazonaws.com/fame-product-renders-dev",
            "renderFallbackUrl": "https://9v2re8nkt4.execute-api.us-east-1.amazonaws.com/dev/ImagePreview"
          }
        }
      }
    }
  },
